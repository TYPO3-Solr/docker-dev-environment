{
    "repositories": [
        { "type": "path", "url": "packages/*" },
        { "type": "composer", "url": "https://composer.typo3.org/" }
    ],
    "name": "apacke-solr-for-typo3/development-distribution",
    "description" : "Distribution for development of EXT:solr based on helhum/typo3-distribution",
    "license": "GPL-2.0+",
    "require": {
        "helhum/typo3-console": "^5.2",

        "typo3/minimal": "^9.2",
        "typo3/cms-about": "^9.2",
        "typo3/cms-belog": "^9.2",
        "typo3/cms-beuser": "^9.2",
        "typo3/cms-documentation": "^9.2",
        "typo3/cms-felogin": "^9.2",
        "typo3/cms-fluid-styled-content": "^9.2",
        "typo3/cms-form": "^9.2",
        "typo3/cms-impexp": "^9.2",
        "typo3/cms-info": "^9.2",
        "typo3/cms-rte-ckeditor": "^9.2",
        "typo3/cms-setup": "^9.2",
        "typo3/cms-sys-note": "^9.2",
        "typo3/cms-t3editor": "^9.2",
        "typo3/cms-tstemplate": "^9.2",
        "typo3/cms-viewpage": "^9.2",
        "typo3/cms-filemetadata": "^9.2",

        "apache-solr-for-typo3/solr": "dev-master",
        "apache-solr-for-typo3/solrfluidexample": "dev-master",

        "dkd/apache-solr-for-typo3-sitepackage": "@dev",
        "apache-solr-for-typo3/tika": "dev-master"
    },
    "require-dev": {
        "typo3/testing-framework": "^1.2.2 || ^2.0.1"
    },
    "extra": {
        "typo3/cms": {
            "root-dir": "web",
            "web-dir": "web"
        }
    }
}
